{
    "manifest_version": 2,
    "name": "Custom New Tab",
    "version": "1.0",
    "description": "This extension replaces the new tab page with a custom one.",
    "permissions": [
      "tabs",
      "storage"
    ],
    "chrome_settings_overrides": {
        "homepage": "newtab.html"
    },
    "chrome_url_overrides": {
      "newtab": "newtab.html"
    },
    "icons": {
      "48": "icons/icon-48.png"
    },
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon-48.png"
    }
  }
  